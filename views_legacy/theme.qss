/* Order Detail */

QLabel#menu_name {
    color: #4A321C;
}

QPushButton#default_button {
    color: #4A321C;
    background: #D8B797;
    border-radius: 20px;
}

QPushButton#default_button:checked {
    background-color: #754926;
    border-radius: 20px;
    color: #F9F5F0;
}

QLabel#sweetness_label {
    color: #754926;
}

QLabel#addOn_label {
    color: #754926;
}

