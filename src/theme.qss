QFrame#order_line {
    background-color: #D8B797;
}

QLabel#default_label {
    color: #4A321C;
    background:transparent;
}

QPushButton#order_button {
    background-color: #754926;
    color: #F9F5F0;
    border: 5px solid #754926;
    border-radius: 20px;
}

/* Menu List Item*/
QWidget#menu_list_widget {
    background: #D8B797;
}

QPushButton#list_button {
    color: #4A321C;
    background: transparent;
    text-align:left;
}

QFrame#white_line {
    border: 3px solid rgba(249, 245, 240, 0.5);
}

/* Order Detail */

QFrame#order_frame {
    background-color: #F9F5F0;
    border: 5px solid #754926;
    border-radius: 20px;
}

QLabel#menu_name {
    color: #4A321C;
    background: transparent;
}

QPushButton#default_button {
    color: #4A321C;
    background: #D8B797;
    border-radius: 20px;
}

QPushButton#default_button:checked {
    background-color: #754926;
    border-radius: 20px;
    color: #F9F5F0;
}

QLabel#sweetness_label {
    color: #754926;
}

QLabel#addOn_label {
    color: #754926;
}

QPushButton#cancel_add_button {
    color: #F9F5F0;
    background: #4A321C;
    border-radius: 20px;
}

QPushButton#cancel_add_button:hover {
    background-color: #754926;
    border-radius: 20px;
    color: #F9F5F0;
}

QFrame#admin_frame {
    background-color: #F9F5F0;
    border: 5px solid #4A321C;
    border-radius: 20px;
}

