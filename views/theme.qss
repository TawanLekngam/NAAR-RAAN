/* 
    application stylesheet
*/

QWidget {
    background-color: #F9F5F0;
}

QLabel#label_username, #label_password {
    background-color: transparent;
    font-family: "Dongle";
    font-weight: bold;
    color: #4A321C;
}

QLineEdit#lineEdit_username, #lineEdit_password {
    font-family: "Dongle";
    font-weight: bold;
    padding-left: 30px;
    padding-right: 30px;
    color: #4A321C;
    background: #D8B797;
    border-radius: 20px;
}

QPushButton#pushButton {
    background-color: #754926;
    width: 200px;
    height: 80px;
    padding-left: 30px;
    padding-right: 30px;
    border: 5px solid #754926;
    border-radius: 20px;
}

QLabel#menu_label {
    background-color: transparent;
    color: #4A321C;
}

QFrame#menu_frame {
    background-color: #D8B797;
    border-radius: 20px;
}

QLineEdit#search_bar {
    padding-left: 30px;
    padding-right: 30px;
    color: #4A321C;
    background: #F9F5F0;
    border: 5px solid #754926;
    border-radius: 20px;
}

